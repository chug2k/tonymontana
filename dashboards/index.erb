      evt.preventDefault();
      $.post('contact', $(this).serializeArray()).done(function() {
        alert('Thanks for signing up! You\'ll hear from us really soon.');
      }).fail(function() {
        alert('Yikes! Something went wrong. Please send charles@fitbookr.com an email pronto.')
