<% content_for :title do %>fitstack demo<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="welcome" data-view="Text"
           data-title="Welcome to FitStack"
           data-text="PenAndPaper Inc."
           data-moreinfo="Questions? support@fitstack.co or 650-398-0FIT.">

      </div>
    </li>

    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="classes_taken" data-view="Meter" data-title="Classes Taken" data-min="0" data-max="40" data-moreinfo="this week"></div>
    </li>
    <li data-row="1" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="biggest_class" data-view="Pictogram" data-image="/pixel_dude.png" data-title="Biggest Class" data-value="8"></div>
    </li>
    <li data-row="1" data-col="4" data-sizex="1" data-sizey="2">
      <div data-id="leaderboard" data-view="List" data-unordered="false" data-title="Leaderboard" data-moreinfo="# of attendances"></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="attendance_by_class"
           data-view="Chartjs"
           data-title="Class Attendance"
           data-type="bar"
           style="background-color:#ff9618">
      </div>
    </li>


    <li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="attendance_by_group"
           data-view="Chartjs"
           data-type="polar"
           data-title="By Group"
           style="background-color:#3AAE40">
      </div>
    </li>
    <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="calories_burned" data-view="Number" data-title="Calories Burned" data-moreinfo="vs last week"></div>
    </li>
    <li data-row="3" data-col="2" data-sizex="2" data-sizey="1">
      <div data-id="distance_traveled"
           data-view="Americamap"
           data-value="927.04 miles"
           data-title="Calories Burned"
           data-image="/usa_topo_small.png">

     </div>
    </li>
    <li data-row="3" data-col="4" data-sizex="1" data-sizey="1">
      <div data-id="steps_taken" data-view="Number" data-title="Steps Taken" data-moreinfo="vs last week"></div>
    </li>

    <li data-row="4" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="cost_month" data-view="Number" data-title="Investment" data-moreinfo="this month" data-prefix="$"></div>
    </li>
    <li data-row="4" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="cost_calorie" data-view="Number" data-title="Cost per Calorie" data-moreinfo="this month" data-prefix="$"></div>
    </li>


  </ul>
</div>